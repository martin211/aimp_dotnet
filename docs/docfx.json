{
  "metadata": [
    {
      "src": [
        {
          "files": [ "/src/AIMP.SDK/AIMP.SDK.csproj" ],
          "exclude": [ "**/obj/**", "**/bin/**" ],
          "cwd": ".."
        }
      ],
      "dest": "apidoc"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
		"src": "apidoc",
		"dest": "api"
      },
	  {
        "files": "RELEASENOTE.md",
        "src": ".."
      },
      {
        "files": [ "tutorial/**/*.{md,yml}", "*.{md,yml}" ]
      }	  
    ],
    "resource": [
      {
        "files": [ "images/**" ],
        "exclude": [ "_site/**", "**/obj/**" ]
      }
    ],
	"overwrite": "apispec/*.md", 
	"globalMetadata": {
      "_appTitle": "Aimp DotNet SDK documentation",
      "_gitContribute": {
        "branch": "develop"
      }
    },
	"postProcessors": ["ExtractSearchIndex"],
    "fileMetadata": {
      "priority": {
        "**.md": 2.5
      }
    },
    "dest": "_site",
    "template": [
		"default",
		"template"
    ]
  }
}